#define _GNU_SOURCE

#include <stdio.h>
#include <stdlib.h>
#include <string.h>

int main(void) {
  char* line = NULL;
  size_t length = 0;
  

  printf("Please enter some text: ");
  ssize_t lines = getline(&line, &length, stdin);
  
  printf("\nTokens:\n");

  char* word = strtok(line, " ");
  while (word != NULL){
  printf("%s\n", word);
  }

  
  free(line);
  return 0;
}
